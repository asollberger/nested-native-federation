{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "pnpm"
    },
    "newProjectRoot": "projects",
    "projects": {
        "home": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/home",
            "sourceRoot": "projects/home/src",
            "prefix": "hm",
            "architect": {
                "build": {
                    "builder": "@angular-architects/native-federation:build",
                    "options": {
                        "polyfills": []
                    },
                    "configurations": {
                        "production": {
                            "target": "home:esbuild:production"
                        },
                        "development": {
                            "target": "home:esbuild:development",
                            "dev": true,
                            "fileReplacements": [
                                {
                                    "replace": "projects/home/src/environments/environment.ts",
                                    "with": "projects/home/src/environments/environment.development.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-architects/native-federation:build",
                    "options": {
                        "target": "home:serve-original:development",
                        "rebuildDelay": 0,
                        "dev": true
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "home:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": ["zone.js", "zone.js/testing"],
                        "tsConfig": "projects/home/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["projects/home/src/favicon.ico", "projects/home/src/assets"],
                        "styles": ["projects/home/src/styles.scss"],
                        "scripts": []
                    }
                },
                "esbuild": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/home",
                        "index": "projects/home/src/index.html",
                        "polyfills": ["zone.js", "es-module-shims"],
                        "tsConfig": "projects/home/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["projects/home/src/favicon.ico", "projects/home/src/assets"],
                        "styles": ["projects/home/src/styles.scss"],
                        "scripts": [],
                        "browser": "projects/home/src/main.ts"
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve-original": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "home:esbuild:production"
                        },
                        "development": {
                            "buildTarget": "home:esbuild:development",
                            "proxyConfig": "proxy.conf.json",
                            "port": 4401
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        },
